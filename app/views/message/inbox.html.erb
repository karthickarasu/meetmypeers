<h3> Incoming meeting Request</h3>
<div>
<ul>
<li><%= link_to  'Inbox',inbox_path %></li>
 <li><%= link_to 'Outbox',outbox_path %></li>
</ul>
 </div>

<div>
<% if @message.present? %>
<table class="curvedEdges" border="1">
<tr>
<th>From</th>
<th>subject</th>
<th>Date Received</th>
<th>Reply</th>

</tr>
<% i=0 %>
	<% @message.each do |q| %>
       <tr>
           <td><%=@rec[i]%><br></td>
	   <td><%= link_to q.subject,"/msgin/#{q.id}" %><br></td>
           <td><%=q.created_at%><br></td>
           <td><%= link_to q.email,"/reply/#{q.id}" %></td></td>
       </tr>
 <% i=i+1 %>
       <% end %>


</table>




<% else %>
       <p> No inbox message </p>
<% end %>



</div>

